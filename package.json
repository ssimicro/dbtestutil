{
  "name": "dbtestutil",
  "version": "6.0.0",
  "description": "Launches an isolated instance of MySQL",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "jshint *.js; cp -n test/db.conf.example test/db.conf || true",
    "test": "mocha -R spec test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ssimicro/dbtestutil"
  },
  "author": "SSi Micro Ltd <propellorheads@ssimicro.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ssimicro/dbtestutil/issues"
  },
  "homepage": "https://github.com/ssimicro/dbtestutil",
  "dependencies": {
    "async": "^3.2.5",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mysql": "^2.18.1",
    "uuid": "^9.0.1",
    "ssi-logger": "^6.1.0"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "jshint": "^2.13.6",
    "mocha": "^10.2.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "globalstrict": true,
    "immed": true,
    "indent": 4,
    "moz": true,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "trailing": true,
    "undef": true,
    "smarttabs": true,
    "strict": true,
    "validthis": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  }
}
